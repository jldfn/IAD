<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD
  Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
 
<web-app>
 
  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>LabPack.ControllerServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Checker</servlet-name>
    <servlet-class>LabPack.AreaCheckServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/Controller</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
    <servlet-name>Checker</servlet-name>
    <url-pattern>/AreaCheckServlet</url-pattern>
  </servlet-mapping>

</web-app>